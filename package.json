{
  "name": "svelte-native-nativescript-ui",
  "version": "0.9.0",
  "description": "Svelte Native support for Nativescript UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halfnelson/svelte-native-nativescript-ui.git"
  },
  "keywords": [
    "sveltejs",
    "nativescript",
    "nativescript-ui",
    "svelte-native"
  ],
  "private": true,
  "author": "David Pershouse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halfnelson/svelte-native-nativescript-ui/issues"
  },
  "homepage": "https://github.com/halfnelson/svelte-native-nativescript-ui#readme",
  "peerDependencies": {
    "svelte-native": "file:../svelte-native/dist",
    "@nativescript/core": "~7.0.0"
  },
  "scripts": {
    "clean": "npx rimraf ./dist/*",
    "build": "npm run clean && rollup -c && node ./scripts/create-pkg.js",
    "watch": "rollup -cw",
